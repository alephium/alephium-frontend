{
  "name": "@alephium/explorer",
  "description": "The Alephium explorer frontend",
  "version": "1.9.1",
  "private": true,
  "scripts": {
    "start": "turbo start:app",
    "start:app": "vite",
    "build": "tsc && vite build --outDir build",
    "build:gh-pages": "pnpm run build -- --base=/explorer/",
    "ci:build": "turbo run build",
    "ci:build:gh-pages": "turbo run build:gh-pages",
    "init-docker": "node docker/init-server-data.js",
    "serve": "pnpm init-docker && serve -s build -p 3000",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --fix --ext .ts,.tsx",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\" --log-level silent",
    "clean": "rm -rf build",
    "clean:node-modules": "rm -rf node_modules"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@alephium/shared": "workspace:*",
    "@alephium/shared-crypto": "workspace:*",
    "@alephium/shared-react": "workspace:*",
    "@alephium/token-list": "^0.0.12",
    "@alephium/web3": "0.38.0",
    "@tanstack/query-sync-storage-persister": "4.32.0",
    "@tanstack/react-query": "4.32.0",
    "@tanstack/react-query-persist-client": "^4.32.0",
    "@yornaath/batshit": "^0.9.0",
    "apexcharts": "^3.35.0",
    "colord": "^2.9.3",
    "dayjs": "^1.10.7",
    "framer-motion": "^10.11.6",
    "i18next": "^23.4.6",
    "lodash": "^4.17.21",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.2.1",
    "react-icons": "^4.10.1",
    "react-is": "^18.2.0",
    "react-middle-ellipsis": "^1.2.1",
    "react-page-visibility": "^7.0.0",
    "react-router-dom": "^6.3.0",
    "react-tooltip": "^5.25.1",
    "styled-components": "^6.1.8",
    "styled-normalize": "^8.0.7",
    "stylis": "^4.0.0"
  },
  "devDependencies": {
    "@alephium/eslint-config": "workspace:*",
    "@alephium/typescript-config": "workspace:*",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.10.1",
    "@types/react": "~18.2.14",
    "@types/react-dom": "^18.2.4",
    "@types/react-page-visibility": "^6.4.1",
    "@types/react-router-dom": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.0.0",
    "esbuild": "^0.17.11",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "happy-dom": "^7.6.6",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "serve": "^14.1.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.2",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.31.0"
  }
}
