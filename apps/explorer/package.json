{
  "name": "@alephium/explorer",
  "description": "The Alephium explorer frontend",
  "version": "1.12.0",
  "private": true,
  "scripts": {
    "start": "turbo start:app",
    "start:app": "vite",
    "build": "tsc && vite build --outDir build",
    "build:gh-pages": "pnpm run build -- --base=/explorer/",
    "ci:build": "turbo run build",
    "ci:build:gh-pages": "turbo run build:gh-pages",
    "init-docker": "node docker/init-server-data.js",
    "serve": "pnpm init-docker && serve -s build -p 3000",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --fix --ext .ts,.tsx",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\" --log-level silent",
    "clean": "rm -rf build .turbo",
    "clean:node-modules": "rm -rf node_modules"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@alephium/shared": "workspace:*",
    "@alephium/shared-crypto": "workspace:*",
    "@alephium/shared-react": "workspace:*",
    "@alephium/token-list": "^0.0.21",
    "@alephium/web3": "2.0.4",
    "@reduxjs/toolkit": "^1.9.1",
    "@tanstack/query-sync-storage-persister": "^5.81.2",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-query-persist-client": "^5.81.2",
    "@yornaath/batshit": "^0.10.1",
    "apexcharts": "^3.35.0",
    "axios": "^1.12.0",
    "colord": "^2.9.3",
    "dayjs": "^1.10.7",
    "framer-motion": "^12.19.1",
    "i18next": "^23.4.6",
    "i18next-browser-languagedetector": "^8.0.0",
    "lodash": "^4.17.21",
    "qrcode.react": "^3.1.0",
    "react": "~19.0.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "~19.0.0",
    "react-i18next": "^15.5.3",
    "react-icons": "^4.10.1",
    "react-intersection-observer": "^9.10.3",
    "react-is": "^19.0.0",
    "react-middle-ellipsis": "^1.2.1",
    "react-page-visibility": "^7.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.3",
    "react-tooltip": "^5.25.1",
    "redux": "^4.2.0",
    "styled-components": "^6.1.19",
    "styled-normalize": "^8.0.7",
    "stylis": "^4.0.0"
  },
  "devDependencies": {
    "@alephium/eslint-config": "workspace:*",
    "@alephium/typescript-config": "workspace:*",
    "@testing-library/jest-dom": "^5.14.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^20.14.0",
    "@types/react": "~19.0.14",
    "@types/react-dom": "~19.1.6",
    "@types/react-page-visibility": "^6.4.1",
    "@types/react-router-dom": "^5.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "esbuild": "^0.25.0",
    "eslint": "^8.48.0",
    "happy-dom": "^7.6.6",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "serve": "^14.1.2",
    "typescript": "~5.8.3",
    "vite": "^5.4.19",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^2.1.9"
  }
}
