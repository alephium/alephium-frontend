{
  "name": "@alephium/keyring",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "lint:fix": "eslint . --fix --ext .ts",
    "test": "jest --useStderr --silent=false --verbose=true --config jestconfig.json --passWithNoTests",
    "test:watch": "pnpm run test -- --watch",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\" --log-level silent",
    "clean": "rm -rf dist",
    "clean:node-modules": "rm -rf node_modules",
    "typecheck": "tsup"
  },
  "dependencies": {
    "@alephium/shared": "workspace:*",
    "@alephium/shared-crypto": "workspace:*",
    "@alephium/web3": "0.29.1",
    "@metamask/scure-bip39": "^2.1.1",
    "@scure/bip32": "^1.4.0",
    "blakejs": "^1.1.1",
    "ethereum-cryptography": "^2.1.2"
  },
  "devDependencies": {
    "@alephium/eslint-config": "workspace:*",
    "@alephium/typescript-config": "workspace:*",
    "@types/jest": "^29.4.0",
    "@types/node": "^16.10.1",
    "@types/rewire": "^2.5.28",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.4.3",
    "rewire": "^6.0.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.3.1",
    "typescript": "^5.2.2"
  }
}
