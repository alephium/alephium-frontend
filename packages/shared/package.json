    "bip39": "^3.0.4",
    "blakejs": "^1.1.1",
    "cross-fetch": "^3.1.5",
    "crypto-js": "^4.1.1",
    "elliptic": "^6.5.4"
  },
  "devDependencies": {
    "@alephium/cli": "^0.10.2",
    "@alephium/web3-test": "^0.10.2",
    "@types/crypto-js": "^4.1.1",
    "@types/elliptic": "^6.4.13",
    "@types/jest": "^29.4.0",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^16.7.8",
    "@types/rewire": "^2.5.28",
