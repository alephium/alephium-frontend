{
  "name": "@alephium/shared",
  "version": "0.7.11",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "watch-dev": "tsup --watch",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "lint:fix": "eslint . --fix --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\" --log-level silent",
    "clean": "rm -rf dist .turbo coverage",
    "clean:node-modules": "rm -rf node_modules",
    "typecheck": "tsup"
  },
  "dependencies": {
    "@alephium/token-list": "^0.0.21",
    "@alephium/walletconnect-provider": "2.0.3",
    "@alephium/web3": "2.0.4",
    "@metamask/scure-bip39": "^2.1.1",
    "@reduxjs/toolkit": "^1.9.1",
    "@walletconnect/sign-client": "2.21.3",
    "@walletconnect/types": "2.21.3",
    "@walletconnect/utils": "2.21.3",
    "@yornaath/batshit": "^0.10.1",
    "cross-fetch": "^3.1.5",
    "dayjs": "^1.10.7",
    "fetch-retry": "^5.0.6",
    "lodash": "^4.17.21",
    "p-throttle": "^6.2.0",
    "posthog-js": "^1.52.0",
    "sanitize-html": "^2.13.1"
  },
  "devDependencies": {
    "@alephium/eslint-config": "workspace:*",
    "@alephium/typescript-config": "workspace:*",
    "@types/node": "^20.14.0",
    "@types/sanitize-html": "^2.11.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^8.48.0",
    "tslib": "^2.3.1",
    "typescript": "~5.8.3",
    "vitest": "^2.1.9"
  }
}
