    "path-browserify": "^1.0.1",
    "rewire": "^6.0.0",
    "shelljs": "^0.8.5",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "webpack": "^5.72.0",
