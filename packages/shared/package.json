    "@metamask/scure-bip39": "^2.1.1",
    "@reduxjs/toolkit": "^1.9.1",
    "@walletconnect/sign-client": "2.17.2",
    "@walletconnect/types": "2.17.2",
    "@walletconnect/utils": "2.17.2",
    "@yornaath/batshit": "^0.10.1",
    "cross-fetch": "^3.1.5",
    "dayjs": "^1.10.7",
    "fetch-retry": "^5.0.6",
    "lodash": "^4.17.21",
